{
  "form_types": [
    {
      "form_type_row_id": 1,
      "name": "vehicle_checkout",
      "version": "1.0",
      "is_active": true,
      "description": "Standard vehicle checkout form",
      "form_structure": {
        "fields": [
          {
            "name": "driver_info",
            "type": "object",
            "required": true,
            "fields": [
              {"name": "name", "type": "string", "required": true},
              {"name": "id", "type": "string", "required": true},
              {"name": "contact", "type": "string", "required": true}
            ]
          },
          {
            "name": "purpose",
            "type": "string",
            "required": true
          },
          {
            "name": "expected_return",
            "type": "datetime",
            "required": true
          },
          {
            "name": "condition",
            "type": "string",
            "required": true,
            "options": ["excellent", "good", "fair", "poor"]
          }
        ]
      }
    },
    {
      "form_type_row_id": 2,
      "name": "maintenance_report",
      "version": "1.0",
      "is_active": true,
      "description": "Standard maintenance report form",
      "form_structure": {
        "fields": [
          {
            "name": "maintenance_type",
            "type": "string",
            "required": true,
            "options": ["routine", "repair", "inspection"]
          },
          {
            "name": "parts_used",
            "type": "array",
            "required": false,
            "items": {
              "type": "object",
              "fields": [
                {"name": "part_id", "type": "string", "required": true},
                {"name": "quantity", "type": "number", "required": true}
              ]
            }
          },
          {
            "name": "findings",
            "type": "text",
            "required": true
          },
          {
            "name": "recommendations",
            "type": "text",
            "required": false
          }
        ]
      }
    }
  ],
  "form_mappings": [
    {
      "mapping_row_id": 1,
      "form_type_row_id": 1,
      "major_event_type": "usage",
      "minor_event_type": "dispatch",
      "is_default": true,
      "priority": 1
    },
    {
      "mapping_row_id": 2,
      "form_type_row_id": 2,
      "major_event_type": "maintenance",
      "minor_event_type": "scheduled",
      "is_default": true,
      "priority": 1
    }
  ]
} 